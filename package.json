{
  "name": "blog",
  "packageManager": "yarn@3.3.1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn watch-styles & next dev",
    "watch-styles": "rollup -c rollup.config.mjs --watch",
    "build-styles": "rollup -c rollup.config.mjs",
    "build": "yarn build-styles && next build",
    "start": "next start",
    "deploy": "now",
    "deploy-prod": "now --prod",
    "new-post": "node scripts/new.mjs"
  },
  "dependencies": {
    "@ds-pack/components": "*",
    "@ds-pack/use-refs": "*",
    "@vanilla-extract/css": "*",
    "@vanilla-extract/dynamic": "*",
    "airtable": "0.11.1",
    "nanopop": "2.1.0",
    "next": "canary",
    "prism-react-renderer": "1.2.1",
    "rainbow-sprinkles": "*",
    "react": "experimental",
    "react-dom": "experimental"
  },
  "devDependencies": {
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vanilla-extract/rollup-plugin": "^1.2.0",
    "fast-glob": "^3.2.12",
    "rollup": "^3.7.5",
    "typescript": "4.9.4"
  }
}
