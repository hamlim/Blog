{
  "name": "blog",
  "packageManager": "yarn@3.3.1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn watch-styles & next dev",
    "watch-styles": "rollup -c rollup.config.mjs --watch",
    "build-styles": "rollup -c rollup.config.mjs",
    "build": "yarn build-styles && next build",
    "start": "next start",
    "deploy": "now",
    "deploy-prod": "now --prod",
    "format": "prettier . --write",
    "new-post": "node scripts/new-post.mjs"
  },
  "dependencies": {
    "@ds-pack/components": "1.0.13",
    "@ds-pack/use-refs": "*",
    "@mdx-js/mdx": "latest",
    "@vanilla-extract/css": "*",
    "@vanilla-extract/dynamic": "*",
    "airtable": "0.11.1",
    "eslint-config-next": "latest",
    "highlight.js": "^11.7.0",
    "nanopop": "2.1.0",
    "next": "latest",
    "prism-react-renderer": "1.2.1",
    "rainbow-sprinkles": "*",
    "react": "latest",
    "react-dom": "latest",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "shiki-twoslash": "^3.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vanilla-extract/rollup-plugin": "^1.2.0",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "prettier": "^2.8.1",
    "rollup": "^3.7.5",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-pluginutils": "^2.8.2",
    "typescript": "4.9.4"
  }
}
